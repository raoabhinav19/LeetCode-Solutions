class Solution:
    def xorOperation(self, n: int, start: int) -> int:
        
        nums = [0] * n
               
        for i in range(n):
            nums[i] = start + 2*i
        
        xor = nums[0]
        
        for i in range(1,len(nums)):
            xor = xor^nums[i]
        return xor
